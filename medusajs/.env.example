# MedusaJS Docker (.env example)
# Default ports: Postgres 5432, Redis 6379, Medusa 9000

# Database
POSTGRES_USER=medusa
POSTGRES_PASSWORD=medusa
POSTGRES_DB=medusa
# Internal service URL used by Medusa container
DATABASE_URL=postgres://medusa:medusa@postgres:5432/medusa

# Cache
REDIS_URL=redis://redis:6379

# Runtime
NODE_ENV=development
